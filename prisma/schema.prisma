generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model ac {
  id_ac  String @id @db.VarChar(50)
  id_adm String @db.VarChar(50)

  @@index([id_adm], map: "id_adm")
}

model accepter_suppliment {
  id_boissons   String @db.VarChar(50)
  id_supplement String @db.VarChar(50)

  @@id([id_boissons, id_supplement])
  @@index([id_supplement], map: "id_supplement")
}

model acheter_distributeur {
  id_distributeur String    @id @db.VarChar(50)
  date_achat      DateTime? @db.DateTime(0)
  id_client       String    @db.VarChar(50)

  @@index([id_client], map: "id_client")
}

model adm {
  id_adm  String @id @db.VarChar(50)
  id_sadm String @db.VarChar(50)

  @@index([id_sadm], map: "id_sadm")
}

model ajouter_supplement {
  id_commande         String  @db.VarChar(50)
  id_supplement       String  @db.VarChar(50)
  quantite_supplement String? @db.VarChar(50)

  @@id([id_commande, id_supplement])
  @@index([id_supplement], map: "id_supplement")
}

model am {
  id_am  String @id @db.VarChar(50)
  id_adm String @db.VarChar(50)

  @@index([id_adm], map: "id_adm")
}

model annoces {
  id_annonces     String  @id @db.VarChar(50)
  Durre_affichage String? @db.VarChar(50)
  sex             String? @db.VarChar(50)
  ageMin          Int?
  AgeMax          Int?
  id_anonceur     String  @db.VarChar(50)

  @@index([id_anonceur], map: "id_anonceur")
}

model annoceur {
  id_anonceur      String  @id @db.VarChar(50)
  nom_annonceur    String? @db.VarChar(50)
  prenom_annonceur String? @db.VarChar(50)
  id_ac            String  @db.VarChar(50)

  @@index([id_ac], map: "id_ac")
}

model boissons {
  id_boissons String  @id @db.VarChar(50)
  nom_boisson String? @db.VarChar(50)
}

model client {
  id_client   String  @id @db.VarChar(50)
  nom         String? @db.VarChar(50)
  prenom      String? @db.VarChar(50)
  type_client String? @db.VarChar(50)
  id_adm      String  @db.VarChar(50)

  @@index([id_adm], map: "id_adm")
}

model commande {
  id_commande String    @id @db.VarChar(50)
  date_cmd    DateTime? @db.DateTime(0)
  id_boissons String    @db.VarChar(50)

  @@index([id_boissons], map: "id_boissons")
}

model commune {
  num_commune String  @id @db.VarChar(50)
  nom_commune String? @db.VarChar(50)
  num_willaya Int

  @@index([num_willaya], map: "num_willaya")
}

model consommateur {
  id_consommateur String  @id @db.VarChar(50)
  mail            String? @db.VarChar(50)
  nom             String? @db.VarChar(50)
  prenom          String? @db.VarChar(50)
}

model decideur {
  id_decideur String @id @db.VarChar(50)
  id_adm      String @db.VarChar(50)

  @@index([id_adm], map: "id_adm")
}

model detection_vol {
  id_vol          String    @id @db.VarChar(50)
  date_heure_vol  DateTime? @db.DateTime(0)
  description_vol String?   @db.VarChar(50)
  id_distributeur String    @db.VarChar(50)

  @@index([id_distributeur], map: "id_distributeur")
}

model distributeur {
  id_distributeur     String  @id @db.VarChar(50)
  num_serie           String? @db.VarChar(50)
  Etat                String? @db.VarChar(50)
  localisation        String? @db.VarChar(50)
  id_sadm             String  @db.VarChar(50)
  id_region           String  @db.VarChar(50)
  id_region_dynamique String  @db.VarChar(50)

  @@index([id_region], map: "id_region")
  @@index([id_region_dynamique], map: "id_region_dynamique")
  @@index([id_sadm], map: "id_sadm")
}

model donner_prix {
  id_ac           String  @db.VarChar(50)
  id_distributeur String  @db.VarChar(50)
  id_boissons     String  @db.VarChar(50)
  prix            Float?
  date_prix       String? @db.VarChar(50)

  @@id([id_ac, id_distributeur, id_boissons])
  @@index([id_boissons], map: "id_boissons")
  @@index([id_distributeur], map: "id_distributeur")
}

model facture {
  id_facture String  @id @db.VarChar(50)
  type_carte String? @db.VarChar(50)
}

model faire_commande {
  id_consommateur String @db.VarChar(50)
  id_distributeur String @db.VarChar(50)
  id_commande     String @db.VarChar(50)

  @@id([id_consommateur, id_distributeur, id_commande])
  @@index([id_commande], map: "id_commande")
  @@index([id_distributeur], map: "id_distributeur")
}

model ingredient {
  id_ingredient     String  @id @db.VarChar(50)
  nom_ingredient    String? @db.VarChar(50)
  quantite_restante Float?
}

model paiement {
  id_paiement   String    @id @db.VarChar(50)
  montant       Float?
  date_paiement DateTime? @db.DateTime(0)
  id_facture    String    @unique(map: "id_facture") @db.VarChar(50)
  id_commande   String    @unique(map: "id_commande") @db.VarChar(50)
}

model panne {
  id_panne          String    @id @db.VarChar(50)
  date_heure_panne  DateTime? @db.DateTime(0)
  description_panne String?   @db.VarChar(50)
  id_am             String    @db.VarChar(50)
  id_distributeur   String    @db.VarChar(50)

  @@index([id_am], map: "id_am")
  @@index([id_distributeur], map: "id_distributeur")
}

model pays {
  nom_pays String @id @db.VarChar(50)
}

model preperer_avec {
  id_boissons          String @db.VarChar(50)
  id_ingredient        String @db.VarChar(50)
  quantite_preparation Float?

  @@id([id_boissons, id_ingredient])
  @@index([id_ingredient], map: "id_ingredient")
}

model reclamation {
  id_reclamation String  @id @db.VarChar(50)
  description    String? @db.VarChar(50)
  id_commande    String  @unique(map: "id_commande") @db.VarChar(50)
}

model region {
  id_region   String @id @db.VarChar(50)
  num_commune String @db.VarChar(50)

  @@index([num_commune], map: "num_commune")
}

model region_dynamique {
  id_region_dynamique String  @id @db.VarChar(50)
  centre              String? @db.VarChar(50)
  diamtre             String? @db.VarChar(50)
}

model sadm {
  id_sadm String @id @db.VarChar(50)
}

model supplement {
  id_supplement  String  @id @db.VarChar(50)
  nom_supplement String? @db.VarChar(50)
}

model willaya {
  num_willaya Int     @id
  nom_willaya String? @db.VarChar(50)
  nom_pays    String  @db.VarChar(50)

  @@index([nom_pays], map: "nom_pays")
}
