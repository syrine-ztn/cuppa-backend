{
  "compilerOptions": {
    "module": "commonjs",
    "rootDir": "./",  
    "esModuleInterop": true,
    "target": "es6",
    "moduleResolution": "node",
    "sourceMap": true,
    "outDir": "./dist",
    "baseUrl": "./",
    "paths": {
      "*": ["node_modules/*", "./*"]
    }
  },
  "include": ["./*"],
  "exclude": ["node_modules", "**/*.spec.ts"]
}






